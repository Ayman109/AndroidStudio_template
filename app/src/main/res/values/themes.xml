<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.SimpleAppTemplate" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColor">@color/white</item>


    </style>

    <style name="custome_button_radius" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_radius</item>
        <item name="android:state_pressed">true</item>
        <item name="android:elevation">20dp</item>

    </style>
    <style name="custome_button_round" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_round</item>
        <item name="android:state_pressed">true</item>
        <item name="android:elevation">20dp</item>
    </style>
    <style name="custome_button_circle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_circle</item>
        <item name="android:state_pressed">true</item>
        <item name="android:elevation">20dp</item>
        <item name="android:layout_width">170px</item>
        <item name="android:layout_height">170px</item>

    </style>
    <style name="custome_buttonstyle2_radius" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/buttonstyle2_radius</item>
        <item name="android:state_pressed">true</item>
