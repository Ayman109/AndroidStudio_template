        <item name="android:elevation">20dp</item>
    </style>



    <style name="custome_editstyle1_text" parent="Widget.AppCompat.EditText">
        <item name="android:background">@drawable/editstyle1_text</item>
    </style>


    <style name="custome_editstyle2_text" parent="Widget.AppCompat.EditText">
        <item name="android:background">@drawable/editstyle2_text</item>
    </style>


