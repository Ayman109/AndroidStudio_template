        <item name="android:background">@drawable/buttonstyle2_round</item>
        <item name="android:state_pressed">true</item>
        <item name="android:elevation">20dp</item>
    </style>
    <style name="custome_buttonstyle2_circle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/buttonstyle2_circle</item>
        <item name="android:state_pressed">true</item>
        <item name="android:elevation">20dp</item>
        <item name="android:layout_width">170px</item>
        <item name="android:layout_height">170px</item>

    </style>
    <style name="custome_buttongradien_radius" parent="Widget.AppCompat.Button">
